#include<stdio.h>
#include<stdlib.h>

int main () {
	printf("pick an item which byte should eat : \n1. pizza,Rs 239 \n2. Burger, Rs129 \n3.Pasta,Rs 179 \n4. French Fries,Rs 99 \n5.Sandwich,Rs 149 ");
	int choice=0;
	scanf("%d ,&choice");
	
	switch(choice)
	{
		case 1:
		     printf("you picked pizza,Rs 239");
		     break;
		case 2:
		     printf("you picked burger,Rs 129");
		     break;
		case 3:
             printf("you picked pasta,Rs 179");
             break;
		case 4:
             printf("you picked French Fries,Rs 99");
             break;
		case 5:
		     printf("you picked Sandwich,Rs 149");
			 break;
		default : printf("Invalid Choice");	 	 	 	      
	}
}
